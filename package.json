{
  "name": "@validkeys/ollypop-ts",
  "version": "1.0.2",
  "main": "dist/index.js",
  "type": "commonjs",
  "bin": {
    "ollypop": "dist/cli.js"
  },
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "test": "vitest || exit 0",
    "clear": "rm -rf ./.tscache && rm -rf ./.turbo",
    "preinstall": "npx only-allow pnpm",
    "lint:ts": "npx tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\" \"*.{js,jsx,ts,tsx,json,md}\"",
    "barrel": "tsx src/cli.ts",
    "build": "tsc --outDir dist",
    "prepublishOnly": "pnpm run lint:ts && pnpm run test",
    "example:basic": "cd examples/basic-barrel && tsx ../../src/cli.ts generate",
    "example:directory": "cd examples/directory-barrel && tsx ../../src/cli.ts generate",
    "example:with-extensions": "cd examples/with-extensions && tsx ../../src/cli.ts generate",
    "example:directory-extensions": "cd examples/directory-with-extensions && tsx ../../src/cli.ts generate",
    "example:factory": "cd examples/factory-pattern && tsx ../../src/cli.ts generate",
    "examples": "pnpm run example:basic && pnpm run example:directory && pnpm run example:with-extensions && pnpm run example:directory-extensions && pnpm run example:factory",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "publish-packages": "changeset publish",
    "release": "pnpm run build && pnpm run version-packages && pnpm run publish-packages"
  },
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.js"
  },
  "description": "Automatic TypeScript barrel file generator CLI.",
  "author": "Kyle Davis <validkeys@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/validkeys/ollypop-ts.git"
  },
  "homepage": "https://github.com/validkeys/ollypop-ts#readme",
  "bugs": {
    "url": "https://github.com/validkeys/ollypop-ts/issues"
  },
  "keywords": [
    "barrel",
    "exports",
    "typescript",
    "generator"
  ],
  "files": [
    "dist",
    "src/cli.ts"
  ],
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/node": "22.15.31",
    "prettier": "^3.6.2",
    "tsx": "^4.19.2",
    "typescript": "5.8.3",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "glob": "^11.0.3",
    "inflection": "^3.0.0",
    "zod": "3.25.76"
  },
  "packageManager": "pnpm@9.15.5+sha512.845196026aab1cc3f098a0474b64dfbab2afe7a1b4e91dd86895d8e4aa32a7a6d03049e2d0ad770bbe4de023a7122fb68c1a1d6e0d033c7076085f9d5d4800d4"
}
